{
    "dependencies": {
        "@apollo/server": "^4.10.4",
        "@supabase/supabase-js": "^2.43.6",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "express": "^4.19.2",
        "express-session": "^1.18.0",
        "graphql": "^16.9.0",
        "graphql-scalars": "^1.23.0"
    },
    "devDependencies": {
        "@eddeee888/gcg-typescript-resolver-files": "^0.10.2",
        "@graphql-codegen/cli": "^5.0.2",
        "@prisma/client": "^5.19.1",
        "@snaplet/copycat": "^5.0.0",
        "@snaplet/seed": "^0.97.20",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/node": "^20.16.10",
        "concurrently": "^9.0.1",
        "dotenv": "^16.4.5",
        "nodemon": "^3.1.3",
        "prisma": "^5.19.1",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "tsx": "^4.19.0",
        "typescript": "^5.6.2"
    },
    "name": "server",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "compile": "npm run generate && tsc",
        "build": "tsc --build",
        "start": "npm run compile && node ./dist/index.js",
        "dev": "tsx watch ./src/index.ts",
        "generate": "graphql-codegen --config codegen.yml",
        "postinstall": "npm run compile",
        "watch": "concurrently  --names \"CodeGen,TS,Nodemon\" -c \"bgYellow,bgBlue,bgMagenta\" \"graphql-codegen --watch 'src/**/*.ts'\"  \"tsc -w \" \"nodemon ./dist/index.js \"",
        "watch:dev": "concurrently \"graphql-codegen --watch 'src/**/*.ts'\" \"tsc --watch ./src/index.ts\""
    },
    "prisma": {
        "seed": "tsx prisma/seed.ts"
    },
    "author": "",
    "license": "ISC",
    "description": "",
    "type": "module"
}
